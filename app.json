{
  "expo": {
    "name": "ManageMoney",
    "slug": "ManageMoney",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "managemoney",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#4caf50"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.managemoney.app",
      "infoPlist": {
        "NSFaceIDUsageDescription": "Allow ManageMoney to use Face ID to unlock the app"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#4caf50"
      },
      "package": "com.managemoney.app",
      "permissions": [
        "USE_BIOMETRIC",
        "USE_FINGERPRINT"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow ManageMoney to use Face ID to unlock the app"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
